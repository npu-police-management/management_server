        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nwpu.managementserver.mapper.AccountMapper">

    <resultMap id="BaseResultMap" type="com.nwpu.managementserver.domain.Account">
            <id property="id" column="id" jdbcType="BIGINT"/>
